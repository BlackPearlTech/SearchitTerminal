# Update Check Function

function updateCheck (){
    cd ~
    wget -q https://raw.githubusercontent.com/BlackPearlTech/SearchitTerminal/master/resource/version |
    if [ -f ~/version ]
    then {
        AvailableVersion=$(cat version)
        if [ "$AvailableVersion" == "$version" ]
        then {
                echo""
              }
        else {
            echo " Version $AvailableVersion is availavle. Use \"searchit -u\" to update."
                rm ~/version
            }
        fi
        }
    fi
    if [ -f ~/version ]
    then {
       rm ~/version
    }
    fi

}
